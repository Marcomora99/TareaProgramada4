    main o red
    ifstream archivo("laberintop.txt", ios::in);
    if(!archivo){
        cerr << "No se pudo abrir el archivo de entrada" << endl;
        return 1;
    }
    Laberinto laberinto(archivo);
    
    constructor
     .h
    Laberinto(ifstream& archivo);
    
    .cpp
    Laberinto::Laberinto(ifstream& archivo){       
    int flujoSalida;
    char charFinLinea = ' ';  
    
    //Linea #1
    archivo >> flujoSalida;
    
    //Linea #2   
    archivo >> flujoSalida; 
    while(!archivo.eof()){
        ListaOrdenada lista;
         
        while(!archivo.eof() && charFinLinea != 10){ 
            lista.agregar(flujoSalida);
            archivo >> flujoSalida;
            archivo.get(); 
            charFinLinea = archivo.peek(); 
        }
        
        if(!archivo.eof())
            lista.agregar(flujoSalida);
        
        archivo >> flujoSalida; 
        archivo.get(); 
        charFinLinea = archivo.peek();        
    }
    if(archivo.eof())
        //termino;
}
